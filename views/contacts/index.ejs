<form action="/contacts" method="GET">
    <h2>Контакти</h2>
    <a class="btn btn-dark" href="/contacts/new">Добавити контакт</a>
    <div>
        <table class="table">
            <thead>
                <tr>
                    <th></th>
                    <th scope="col">Контакт</th>
                    <th scope="col">Телефон</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <% contacts.forEach(contact => { %>
            <tbody>
                <td><%= contact.titul %></td>
                <td><%= contact.name %></td>
                <td><%= contact.phoneNumber %> </td>
                <td><a class="btn btn-secondary float-right" href="/contacts/<%= contact.id %>">Edit</a></td>
                <td><a class="btn btn-danger float-left" href="/contacts/<%= contact.id %>/del">Delete</a></td>
    </div>
    <% }) %>
    </tbody>
    </table>
    </div>

</form>
<br>
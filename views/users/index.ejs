<h2>Users</h2>
<form action="/users" method="GET">
    <a href="/users/new">Add new User</a>
    <% users.forEach(user => { %>
        <div class="user_Frame">
            <div><%= user.name %> </div>
            <div><%= user.email %> </div>
            <div><%= user.number %> </div>
            <div><%= user.encryptedPassword %> </div>
            <div><%= user.owner %> </div>
            <div><%= user.receplents %> </div>
            <div><%= user.bill %> </div>
            <div><%= user.area %> </div>
            <div><%= user.adress %> </div>
            <div><%= user.benefits %> </div>
            <div><%= user.role %> </div>
            <% user.imgPath.forEach(img => { %>
                <img class="avatarImg" src="<%= img %> " alt="">
            <% }) %> 
        </div>
    <% }) %> 
</form>
<br>



<form action="/users" method="GET">
    <div>
        <h2>Users</h2>
        <a class="btn btn-dark" href="/users/new">Add new User</a>
        <ul class="list-group">
            <% users.forEach(user => { %>
                <div class="user_Frame">
                    <div class="list-group-item"><a href="/users/<%= user.id %> "><%= user.name %> </a> 
                    <a  class="btn btn-danger float-right" href="/users/<%= user.id %>/del">Delete</a>
                    </div>
                </div>
            <% }) %> 
        </ul>
    </div>
</form>
<br>


